{
  "expo": {
    "name": "Food Fone",
    "slug": "mobile-food-ai",
    "description": "Aplicação desenvolvida com react native",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#682dce"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.projetotcc.projetoapp"
    },
    "android": {
      "package": "com.projetotcc.projetoapp"
    }
  }
}
